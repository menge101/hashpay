<% button = { text: 'Registration List', path: whosecoming_path(id: @event.id)} %>
<div id="event"><%= render partial: 'events/event', locals: { event: @event, button: button } %></div>
<% if @event.rego_allowed? %>
    <div id="rego"><%= render partial: 'events/rego', locals: { event: @event, public_key: @public_key } %></div>
<% end %>
<% if @event.allow_upload? && user_signed_in? && !GpsData.exists?(event_id: @event.id, user_id: current_user.id) %>
  <div id="upload"><%= render partial: 'events/gps_upload', locals: {event: @event } %></div>
<% end %>